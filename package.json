	"scripts": {
		"add-translation": "bun run scripts/add_new_translation.ts",
		"build": "vite build && bun ./scripts/tasksBuild.ts",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "concurrently \"bun run dev:server\" \"bun run dev:docker\"",
		"dev:docker": "docker compose -f ./dev.docker-compose.yml up",
		"dev:server": "while true; do svelte-kit sync && vite; echo 'ðŸ”„ Server crashed, restarting...'; sleep 1; done",
		"format": "prettier --write .",
		"install-git-hooks": "bunx lefthook install",
		"lint": "prettier --check . && eslint .",
		"i18n:check": "i18n-check --source \"de\" --locales ./messages --format \"i18next\"",
		"i18n:validate": "inlang validate --project project.inlang",
		"i18n:translate": "inlang machine translate --project project.inlang",
		"preview": "vite preview",
		"studio": "prisma studio",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest",
		"coverage": "vitest run --coverage"
	},
