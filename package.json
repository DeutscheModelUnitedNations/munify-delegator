{
	"name": "munify-delegator",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently \"bun run dev:server\" \"bun run dev:docker\"",
		"dev:server": "vite",
		"dev:docker": "docker compose -f ./dev.docker-compose.yml up",
