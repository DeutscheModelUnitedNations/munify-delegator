[
  {
    "ClientId": "delegator",
    "Description": "Client for implicit flow",
    "AllowedGrantTypes": [
      "implicit"
    ],
    "AllowAccessTokensViaBrowser": true,
    "RedirectUris": [
      "http://localhost:5173/auth/login-callback"
    ],
    "AllowedScopes": [
      "openid",
      "profile",
      "offline_access",
      "address",
      "email",
      "family_name",
      "gender",
      "given_name",
      "locale",
      "name",
      "phone",
      "preferred_username",
      "urn:zitadel:iam:org:projects:roles",
      "urn:zitadel:iam:user:metadata"
    ],
    "IdentityTokenLifetime": 3600,
    "AccessTokenLifetime": 3600
  }
]